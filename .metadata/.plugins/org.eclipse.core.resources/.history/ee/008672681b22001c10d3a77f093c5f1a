#include "Buzzer.h"

void Implement_Button(){

	unit8 value=Get_Button(BUTTON1);

    if(value==1){
    	PlayNote(40,523251);
    }

    value =Get_Button(BUTTON2);
    if(value==1){
    	PlayNote(40,587733);
    }



    value =Get_Button(BUTTON3);
        if(value==1){
        	PlayNote(40,659.255);
        }



    value =Get_Button(BUTTON4);
    if(value==1){
    	PlayNote(40,698.456);
    }


}




