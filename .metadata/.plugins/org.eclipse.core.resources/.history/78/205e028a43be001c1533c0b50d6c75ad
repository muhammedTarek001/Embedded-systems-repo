#include"GPIO.h"
#include "STD_Types.h"
#include "Utils.h"
#include"Led.h"
#include <avr/io.h>
#include <util/delay.h>
#include "Button.h"
#include "Clock.h"
#include "SEVERAL_TASKS.h"
#include "Seg.h"
#include "LCD.h"
#include "Servo.h"
#include<string.h>

int main() {

	lcd_Init();

	while (1) {

		unit8 pressedKey = detect_Button();
		if (pressedKey == BUTTON4) {
			rotate90();
		}
		else{
             rotate00();
		}

	}
	return 0;
}
