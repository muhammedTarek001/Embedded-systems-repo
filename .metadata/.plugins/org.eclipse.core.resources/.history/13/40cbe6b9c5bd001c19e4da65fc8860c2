#include"GPIO.h"
#include "STD_Types.h"
#include "Utils.h"
#include"Led.h"
#include <avr/io.h>
#include <util/delay.h>
#include "Servo.h"
#include<string.h>

void rotate90(){

	GPIO_WritePin(servo_PORT,servo_PIN,GPIO_HIGH);
	_delay_us(1500);
	GPIO_WritePin(servo_PORT,servo_PIN,GPIO_LOW);

	_delay_ms(2000);
}

void rotate00(){

	GPIO_WritePin(servo_PORT,servo_PIN,GPIO_HIGH);
	_delay_us(2000);
	GPIO_WritePin(servo_PORT,servo_PIN,GPIO_LOW);

	_delay_ms(2000);
}
